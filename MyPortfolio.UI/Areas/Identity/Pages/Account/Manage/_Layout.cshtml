@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = parentLayout == null ? "" : parentLayout.ToString();
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}

<div>
    <h2>Profile</h2>
    <hr />
    <div class="row">
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
